{
    "name": "bakerkretzmar/nova-asset-manager",
    "description": "A Laravel Nova tool and field to manage files and images.",
    "keywords": ["laravel", "nova", "asset", "file", "manager"],
    "homepage": "https://github.com/bakerkretzmar/nova-asset-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "Jacob Baker-Kretzmar",
            "email": "jacob.baker@culturedays.ca",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "7.1"
    },
    "autoload": {
        "psr-4": {
            "Bakerkretzmar\\AssetManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bakerkretzmar\\AssetManager\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bakerkretzmar\\AssetManager\\AssetManagerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
